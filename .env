# ---- Airflow ----
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASS=admin
AIRFLOW_ADMIN_EMAIL=admin@example.com

# ---- Pipeline / Ollama ----
VLLM_BASE_URL=http://127.0.0.1:8001/v1
VLLM_MODEL=skt/A.X-4.0-Light
